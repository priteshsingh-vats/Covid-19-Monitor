<body>
    <style>
        h1 {
            text-align: center;
            font-family: 'Courier New', Courier, monospace;
            background: black;
            color: white;
            padding: 10px;
        }

        footer {
            text-align: center;
            font-family: 'Courier New', Courier, monospace;
            background: black;
            color: white;
            padding-top: 10px;
            padding-bottom: 10px;
        }

        #ac {
            color: gray;
        }

        #cc {
            color: orange;
        }

        #de {
            color: red
        }

        #re {
            color: green;
        }

        #al {
            color: red;
            border: 1px solid red;
            border-radius: 20px;
            padding-left: 5px;
            padding-right: 5px;
        }

        span {
            color: red;
        }

        @media only screen and (max-width:500px) {

            /* For mobile phones: */
            table {
                font-size: 9.6px;
            }
        }
    </style>

    <h1 style="font-size:3.5vw;"> Covid-19 Monitor India <i class="India flag"></i> </h1>

    <button class="btn btn-danger" type="button" disabled>
        <span class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true"></span>
        Live Updates
    </button>
    <br /><br />
    <div class="container">
        <h5> Total Number of Confirmed cases: <span id="cc"><%=p.total_values.confirmed %></span></h5>
        <hr id="q">
        <h5> Total Number of Active cases: <span id="ac"><%= p.total_values.active %></span></h5>
        <hr id="q">
        <h5> Total Number of Deaths cases: <span id="de"><%= p.total_values.deaths %></span></h5>
        <hr id="q">
        <h5> Total Number of Recovered cases: <span id="re"><%= p.total_values.recovered %></span></h5>
        <hr id="q">
        <h5> Total Number of Migrated cases: <span id="re">1</span></h5>
    </div>
    <br>
    <table class="table table-striped">
        <thead class="thead-dark">
            <tr>
                <th scope="col">State</th>
                <th scope="col">Active</th>
                <th scope="col">Confirmed</th>
                <th scope="col">Death</th>
                <th scope="col">Recovered</th>
                <th scope="col">State Code</th>
            </tr>
        </thead>
        <tbody>
            <%Object.keys(p.state_wise).forEach(function(key) {%>
            <tr>
                <% if(key=="Unknown"){%>
                <%}else{%>
                <th scope="row"> <%=key%></th>
                <% }%>
                <%Object.keys(p.state_wise[key]).forEach(function(key1) {%>
                <%if(key1==="active"||key1==="confirmed"||key1==="deaths"||key1==="recovered"||key1==="statecode"){%>
                    <td> <%=p.state_wise[key][key1]%></td>
                <%}else{%>
               
                <%}%>
        <%})%>
            </tr>
            <%})%>
        </tbody>
    </table>
